@model DailyStatement.Models.WorkCategory

@{
    ViewBag.Title = "編輯工作類型";
}

<div class="row">
    <div class="span6 offset3">

@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>編輯工作類型</legend>

        @Html.HiddenFor(model => model.WorkCategoryId)

        <div class="editor-label">
            @Html.LabelFor(model => model.Name)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.Name)
            @Html.ValidationMessageFor(model => model.Name)
        </div>

        <p>
            <input type="submit" value="儲存" class="btn btn-small btn-success" /> @Html.ActionLink("返回", "CategoryIndex", null, new { @class="btn btn-small"})
        </p>
    </fieldset>
}
</div>
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
